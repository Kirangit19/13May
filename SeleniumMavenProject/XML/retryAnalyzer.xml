<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Retry Suit">
	<listeners>
		<listener class-name="analyzer.IretryAnalyzer"/>
	</listeners>
	<test name="RetryFailed">
		<classes>
			<class name="retryAnalyzer.RuntimeRetry"></class>
		</classes>
	</test>
</suite>